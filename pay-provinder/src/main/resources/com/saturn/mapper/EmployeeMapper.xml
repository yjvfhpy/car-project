<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.saturn.mapper.EmployeeMapper">
	<select id="selectAll" resultType="employeeEntity">
		select
		id,empno,name,age,birthday,salary
		from employee
	</select>


	<select id="findPage" resultType="employeeEntity">
		select
		   id,empno,name,age,birthday,salary
		     from employee
	</select>

</mapper>  